{
  "version": "1.0.2",
  "baseUrl": "https://raw.githubusercontent.com/N2con-Inc/PS_Netbird_Master_Script/main/modular/modules",
  "launcher": {
    "version": "1.2.7",
    "file": "netbird.launcher.ps1",
    "description": "Main orchestration launcher"
  },
  "modules": {
    "core": {
      "file": "netbird.core.ps1",
      "version": "1.0.0",
      "required": true,
      "description": "Core installation and logging"
    },
    "version": {
      "file": "netbird.version.ps1",
      "version": "1.1.0",
      "dependencies": ["core"],
      "description": "Version detection, GitHub API, and version compliance"
    },
    "service": {
      "file": "netbird.service.ps1",
      "version": "1.0.0",
      "dependencies": ["core"],
      "description": "Windows service management"
    },
    "registration": {
      "file": "netbird.registration.ps1",
      "version": "1.0.0",
      "dependencies": ["core", "service"],
      "description": "Enhanced registration system"
    },
    "diagnostics": {
      "file": "netbird.diagnostics.ps1",
      "version": "1.0.0",
      "dependencies": ["core", "service"],
      "description": "Status and diagnostics"
    },
    "oobe": {
      "file": "netbird.oobe.ps1",
      "version": "1.0.0",
      "dependencies": ["core", "service", "registration"],
      "description": "OOBE deployment logic"
    },
    "zerotier": {
      "file": "netbird.zerotier.ps1",
      "version": "1.0.0",
      "dependencies": ["core", "service", "registration"],
      "description": "ZeroTier migration"
    }
  },
  "workflows": {
    "Standard": ["core", "version", "service", "registration", "diagnostics"],
    "OOBE": ["core", "version", "service", "oobe"],
    "ZeroTier": ["core", "version", "service", "registration", "zerotier"],
    "Diagnostics": ["core", "diagnostics"]
  }
}
